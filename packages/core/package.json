{
  "name": "@dashbling/core",
  "version": "0.0.1-beta5",
  "author": "Pascal Widdershoven",
  "license": "MIT",
  "bin": {
    "dashbling": "./lib/cli.js"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon --config ./nodemon.json --watch src/ --exec 'bash -c' 'cd example && ts-node ../src/cli.ts start'",
    "prettier": "prettier '**/*.@(js|ts|json)'",
    "test": "yarn run --silent build && jest",
    "test:watch": "jest --watch",
    "test:e2e": "./script/e2e-tests.sh",
    "build": "./node_modules/.bin/tsc -p .",
    "prepare": "yarn run --silent build && chmod +x lib/cli.js"
  },
  "files": [
    "/client/",
    "/example/",
    "/lib/",
    "/src/"
  ],
  "dependencies": {
    "commander": "^2.12.2",
    "date-fns": "^1.29.0",
    "hapi": "17.1.1",
    "hapi-auth-basic": "^5.0.0",
    "inert": "^5.0.1",
    "node-cron": "^1.2.1",
    "node-fetch": "^1.7.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "timezone": "^1.0.13",
    "tracer": "^0.8.11"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.57",
    "@dashbling/build-support": "0.0.1-beta1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^21.2.0",
    "jest": "^21.2.1",
    "nodemon": "^1.13.3",
    "prettier": "^1.9.2",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "((\\.|/)test)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
