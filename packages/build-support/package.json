{
  "name": "@dashbling/build-support",
  "version": "0.0.1-beta.9",
  "author": "Pascal Widdershoven",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc -p .",
    "prepare": "yarn run --silent build",
    "test": "yarn run --silent build && jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "@babel/preset-react": "^7.0.0-beta.34",
    "@dashbling/core": "^0.0.1-beta.9",
    "autoprefixer": "^7.1.6",
    "babel-loader": "^8.0.0-beta.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.9.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.57",
    "ts-node": "^3.3.0",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "transformIgnorePatterns": ["node_modules/(?!(@dashbling)/)"],
    "testRegex": "((\\.|/)test)\\.(js|ts)$",
    "moduleFileExtensions": ["ts", "js"],
    "silent": true
  },
  "engines": {
    "node": ">= 8.9.0"
  }
}
